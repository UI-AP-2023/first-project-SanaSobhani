<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="600.0" prefWidth="877.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.phase2.HistoryPageController">
   <children>
      <ImageView fx:id="backGroundImg" fitHeight="571.0" fitWidth="1034.0" layoutX="127.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@history4.jpg.png" />
         </image>
         <effect>
            <BoxBlur height="11.38" iterations="3" width="8.19" />
         </effect>
      </ImageView>
      <Label fx:id="topic_lbl" layoutX="14.0" layoutY="17.0" text="Your Shopping History" textFill="#069297">
         <font>
            <Font name="Calibri Bold" size="28.0" />
         </font>
      </Label>
      <Button fx:id="homeButton" layoutX="782.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="79.0" prefWidth="85.0" style="-fx-background-radius: 50; -fx-background-color: #fbfbfb;" text=" " textFill="#fbfbfb" />
      <ImageView fx:id="homeImg" fitHeight="50.0" fitWidth="61.0" layoutX="794.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@home3.JPG" />
         </image>
      </ImageView>
      <ScrollPane fx:id="mainScroll" layoutX="14.0" layoutY="77.0" prefHeight="521.0" prefWidth="250.0">
         <content>
            <ListView fx:id="histoyList" prefHeight="526.0" prefWidth="380.0" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
